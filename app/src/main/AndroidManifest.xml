<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bruno.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Hotel"
            android:label="Hotels" />
        <activity
            android:name=".Entertainment_Activity"
            android:label="Entertainment" />
        <activity
            android:name=".Transportation_Activity"
            android:label="Transportation" />
        <activity
            android:name=".Restaurants_Activity"
            android:label="Restaurants">

        </activity>
    </application>

</manifest>